   "source": [
    "#The sum of all numbers multiplied by 3 or 5 for all numbers below 1000\n",
    "s = 0\n",
    "for num in range(1000):\n",
    "    if num % 3 == 0 or num % 5 == 0:\n",
    "        s += num\n",
    "    continue\n",
    "    \n",
    "print(s)"
   ]
